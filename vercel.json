{
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/",
			"has": [
				{
					"type": "missing",
					"value": "/(sitemap\\.xml|robots\\.txt)"
				}
			]
		}
	],
	"headers": [
		{
			"source": "/sitemap.xml",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/xml"
				}
			]
		}
	]
}
